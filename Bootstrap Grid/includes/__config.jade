mixin tagblock()
  .row.text-muted
    .col-sm-4
      .glyphicon.glyphicon-time
      |  3 days ago
    .col-sm-8
      .glyphicon.glyphicon-tags
      strong  Tags:
        a(href="")=  tagblock_var[0]
        | ,
        a(href="")=  tagblock_var[1]
        | ,
        a(href="")=  tagblock_var[2]



mixin mediaobject(title)
  .media
    a.media-left(href="")
      img.img-circle(src="http://placehold.it/60x60" alt="...")

    .media-body
